   "source": [
    "pares <- function(numbers) {\n",
    "    v = c()\n",
    "    for (i in numbers) {\n",
    "        if (i %% 2 == 0) {\n",
    "            v <- c(v,c(i))\n",
    "        }\n",
    "    \n",
    "    }\n",
    "    print(v)\n",
    "}"
   ]
